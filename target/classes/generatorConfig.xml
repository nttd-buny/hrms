<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
  <classPathEntry location="/PATH_TO/mysql-connector-java-5.1.19.jar"/>
  <context id="context1">
    <commentGenerator>
      <property name="suppressDate" value="true" />
    </commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
      connectionURL="jdbc:mysql://localhost/hrms" userId="hrms"
      password="hrms" />
    <javaModelGenerator targetPackage="jp.co.sample.hrms.domain"
      targetProject="hrmsl" />
    <sqlMapGenerator targetPackage="jp.co.sample.hrms.mapper"
      targetProject="hrms/src/main/resources" />
    <javaClientGenerator targetPackage="jp.co.sample.hrms.mapper"
      targetProject="jp.co.sample.hrms" type="XMLMAPPER" />
    <table tableName="member" alias="member"/>
  </context>
</generatorConfiguration>